@{
    ViewBag.Title = "BatchEdit";
}
@using Rookey.Frame.Common;
@using Rookey.Frame.UIOperate;
@{ UIFrameFactory frameFactory = UIFrameFactory.GetInstance(Request);
   Guid moduleId = UIOperate.GetModuleIdByRequest(Request);
   int selectRecords = Request["selectRecords"].ObjToInt();
   int pageRecords = Request["pageRecords"].ObjToInt();}
@Html.Raw(frameFactory.GetBatchEditHTML(moduleId,selectRecords,pageRecords))