@{
    ViewBag.Title = "EditField";
}
@using Rookey.Frame.Common;
@using Rookey.Frame.UIOperate;
@{ UIFrameFactory frameFactory = UIFrameFactory.GetInstance(Request);
   Guid moduleId = UIOperate.GetModuleIdByRequest(Request);
   string fieldName = Request["fieldName"].ObjToStr();
   Guid recordId = Request["recordId"].ObjToGuid();}
@Html.Raw(frameFactory.GetEditFieldHTML(moduleId, fieldName, recordId))
